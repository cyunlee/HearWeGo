<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <!-- 로고 -->
    <RelativeLayout
        android:id="@+id/logo"
        android:layout_width="411dp"
        android:layout_height="823dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="216dp"
        android:layout_marginTop="1038dp"
        android:background="@drawable/background"
        tools:ignore="MissingConstraints"
        >

        <!-- Status Bar -->
        <RelativeLayout
            android:id="@+id/status_bar"
            android:layout_width="411dp"
            android:layout_height="32dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:background="@drawable/status_bar"
            tools:ignore="MissingConstraints" >
        </RelativeLayout>

        <!-- Frame_Logo -->
        <RelativeLayout
            android:id="@+id/container_logo"
            android:layout_width="149dp"
            android:layout_height="101dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="131dp"
            android:layout_marginTop="301dp"
            android:background="@drawable/hear_we_go" />

        <!-- Frame_google_login -->
        <ImageButton
            android:id="@+id/google_login"
            android:layout_width="395dp"
            android:layout_height="90dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="603dp"
            android:layout_marginBottom="130dp"
            android:background="@drawable/google_login"
            android:clickable="true"
            tools:ignore="MissingConstraints" />
    </RelativeLayout>



</androidx.constraintlayout.widget.ConstraintLayout>