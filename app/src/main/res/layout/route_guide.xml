<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#A8A8A8">
    <!-- Frame 29 -->

    <RelativeLayout
        android:id="@+id/frame_29"
        android:layout_width="0dp"
        android:layout_height="92dp"
        android:layout_alignParentLeft="false"
        android:layout_alignParentTop="true"
        android:background="@drawable/frame_29"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

        <TextView
            android:id="@+id/destination_text"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:gravity="center"
            android:text="목적지 주소"
            android:textColor="#FFE500"
            android:textSize="36sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />
    </RelativeLayout>

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="409dp"
        android:layout_height="81dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">


        <ImageButton
            android:id="@+id/previous"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@drawable/previous_button"/>

        <ImageButton
            android:id="@+id/home"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@drawable/home_button"/>
    </LinearLayout>

    <ImageButton
        android:id="@+id/button2_nowgps"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="29dp"
        android:background="@drawable/now_gps"
        android:gravity="center"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/button_back"
        style="@android:style/Widget.ImageButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="30dp"
        android:layout_marginRight="30dp"
        android:layout_marginBottom="30dp"
        android:background="@drawable/skip_next"
        app:icon="@drawable/prev"
        app:iconGravity="textEnd"
        app:iconTint="#FFE500"
        app:layout_constraintBottom_toTopOf="@+id/button2_nowgps"
        app:layout_constraintEnd_toStartOf="@+id/button_pause"
        app:srcCompat="@drawable/prev" />

    <ImageButton
        android:id="@+id/button_pause"
        style="@android:style/Widget.ImageButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="30dp"
        android:background="@drawable/skip_next"
        app:icon="@drawable/pause"
        app:iconGravity="textEnd"
        app:iconPadding="0dp"
        app:iconTint="#FFE500"
        app:layout_constraintBottom_toTopOf="@+id/button2_nowgps"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/pause"
        tools:ignore="VectorDrawableCompat" />

    <ImageButton
        android:id="@+id/button_forth"
        style="@android:style/Widget.ImageButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginLeft="30dp"
        android:layout_marginBottom="30dp"
        android:background="@drawable/skip_next"
        app:icon="@drawable/next"
        app:iconGravity="textEnd"
        app:iconTint="#FFE500"
        app:layout_constraintBottom_toTopOf="@+id/button2_nowgps"
        app:layout_constraintStart_toEndOf="@+id/button_pause"
        app:srcCompat="@drawable/next" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0M"
        android:textColor="#FFE500"
        android:textSize="64sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <FrameLayout
        android:id="@+id/frameLayout"
        android:layout_width="220dp"
        android:layout_height="166dp"
        android:layout_marginBottom="400dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frame_29">

        <ImageView
            android:id="@+id/left_point"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="invisible"
            app:srcCompat="@drawable/left_point" />

        <ImageView
            android:id="@+id/right_point"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="invisible"
            app:srcCompat="@drawable/right_point" />

        <ImageView
            android:id="@+id/straight_point"
            android:layout_width="match_parent"
            android:layout_height="211dp"
            android:soundEffectsEnabled="false"
            android:visibility="visible"
            app:srcCompat="@drawable/straight_point" />

    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>